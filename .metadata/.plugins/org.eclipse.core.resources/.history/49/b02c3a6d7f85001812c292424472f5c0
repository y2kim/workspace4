<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="Messages">
    	<select id="selectAll" resultType="typeAliases">
    	<!-- typeAliases 로  resultType 줄일수 있다  -->
    		select * from messages
    	</select>
    </mapper>
    
<!-- ; 붙이지 말것   언어의 독립성이 강하다  -->